    <!-- Object Property Characteristics -->
    
    <!-- appartientÀ is Functional (a vessel belongs to exactly one company) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#appartientÀ"/>
    </FunctionalObjectProperty>
    
    <!-- commandéPar is Functional (a vessel has exactly one captain) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#commandéPar"/>
    </FunctionalObjectProperty>
    
    <!-- employéPar is Functional (a captain works for exactly one company) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#employéPar"/>
    </FunctionalObjectProperty>
    
    <!-- transportéPar is Functional (a cargo is transported by exactly one vessel) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#transportéPar"/>
    </FunctionalObjectProperty>
    
    <!-- depuisPort is Functional (a route has exactly one departure port) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#depuisPort"/>
    </FunctionalObjectProperty>
    
    <!-- versPort is Functional (a route has exactly one arrival port) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#versPort"/>
    </FunctionalObjectProperty>
    
    <!-- utiliséPar is Functional (an engine is used by exactly one vessel) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#utiliséPar"/>
    </FunctionalObjectProperty>
    
    <!-- effectuéPar is Functional (a route is performed by exactly one vessel) -->
    <FunctionalObjectProperty>
        <ObjectProperty IRI="#effectuéPar"/>
    </FunctionalObjectProperty>
    
