    <!-- SWRL Rules -->
    
    <!-- Example SWRL Rules -->
    
    <!-- Rule 1: If a captain commands a vessel and that vessel belongs to a company, 
         then the captain is employed by that company -->
    <DLSafeRule>
        <Body>
            <ClassAtom>
                <Class IRI="#Capitaine"/>
                <Variable IRI="urn:swrl:var#c"/>
            </ClassAtom>
            <ClassAtom>
                <Class IRI="#Navire"/>
                <Variable IRI="urn:swrl:var#n"/>
            </ClassAtom>
            <ClassAtom>
                <Class IRI="#CompagnieMaritime"/>
                <Variable IRI="urn:swrl:var#cm"/>
            </ClassAtom>
            <ObjectPropertyAtom>
                <ObjectProperty IRI="#commandéPar"/>
                <Variable IRI="urn:swrl:var#n"/>
                <Variable IRI="urn:swrl:var#c"/>
            </ObjectPropertyAtom>
            <ObjectPropertyAtom>
                <ObjectProperty IRI="#appartientÀ"/>
                <Variable IRI="urn:swrl:var#n"/>
                <Variable IRI="urn:swrl:var#cm"/>
            </ObjectPropertyAtom>
        </Body>
        <Head>
            <ObjectPropertyAtom>
                <ObjectProperty IRI="#employéPar"/>
                <Variable IRI="urn:swrl:var#c"/>
                <Variable IRI="urn:swrl:var#cm"/>
            </ObjectPropertyAtom>
        </Head>
    </DLSafeRule>
    
    <!-- Rule 2: If a route is performed by a vessel that transports a cargo,
         then that cargo is on that route -->
    <DLSafeRule>
        <Body>
            <ClassAtom>
                <Class IRI="#Trajet"/>
                <Variable IRI="urn:swrl:var#t"/>
            </ClassAtom>
            <ClassAtom>
                <Class IRI="#Navire"/>
                <Variable IRI="urn:swrl:var#n"/>
            </ClassAtom>
            <ClassAtom>
                <Class IRI="#Cargaison"/>
                <Variable IRI="urn:swrl:var#cg"/>
            </ClassAtom>
            <ObjectPropertyAtom>
                <ObjectProperty IRI="#effectuéPar"/>
                <Variable IRI="urn:swrl:var#t"/>
                <Variable IRI="urn:swrl:var#n"/>
            </ObjectPropertyAtom>
            <ObjectPropertyAtom>
                <ObjectProperty IRI="#transportéPar"/>
                <Variable IRI="urn:swrl:var#cg"/>
                <Variable IRI="urn:swrl:var#n"/>
            </ObjectPropertyAtom>
        </Body>
        <Head>
            <!-- Define a new property 'surTrajet' (on route) -->
            <ObjectPropertyAtom>
                <ObjectProperty IRI="#surTrajet"/>
                <Variable IRI="urn:swrl:var#cg"/>
                <Variable IRI="urn:swrl:var#t"/>
            </ObjectPropertyAtom>
        </Head>
    </DLSafeRule>
    
    <!-- Some example individuals -->
    
    <!-- Example of a shipping company -->
<Declaration>
    <NamedIndividual IRI="#CompagnieTunisienneDeNavigation"/>
</Declaration>
<ClassAssertion>
    <Class IRI="#CompagnieMaritime"/>
    <NamedIndividual IRI="#CompagnieTunisienneDeNavigation"/>
</ClassAssertion>
<DataPropertyAssertion>
    <DataProperty IRI="#IdCompanie"/>
    <NamedIndividual IRI="#CompagnieTunisienneDeNavigation"/>
    <Literal datatypeIRI="&xsd;string">COMP_001</Literal>
</DataPropertyAssertion>
<DataPropertyAssertion>
    <DataProperty IRI="#NomCompanie"/>
    <NamedIndividual IRI="#CompagnieTunisienneDeNavigation"/>
    <Literal datatypeIRI="&xsd;string">Compagnie Tunisienne de Navigation</Literal>
</DataPropertyAssertion>
<DataPropertyAssertion>
    <DataProperty IRI="#NombreNavire"/>
