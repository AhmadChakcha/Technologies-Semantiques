    <Literal datatypeIRI="&xsd;integer">50</Literal>
</DataPropertyAssertion>
<DataPropertyAssertion>
    <DataProperty IRI="#AnnéeCréation"/>
    <NamedIndividual IRI="#CompagnieTunisienneDeNavigation"/>
    <Literal datatypeIRI="&xsd;integer">1955</Literal>
</DataPropertyAssertion>

