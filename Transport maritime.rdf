    </ClassAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#IdNavire"/>
        <NamedIndividual IRI="#MaerskMcKinneyMoller"/>
        <Literal datatypeIRI="&xsd;string">NAV_001</Literal>
    </DataPropertyAssertion>
